<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= category%>Menu</title>
  </head>
  <body>
    <%- include('./partials/nav') %>
    <h1><%= category%> menu</h1>

    <% if(menuItems.length === 0){%>
    <p>no items</p>

    <% } else{%>
    <ul>
      <% menuItems.forEach(item =>{%>

      <li>
        <h2><%= item.name %></h2>
        Price: $<%= item.price %><br />
        Rating: <%= item.rating %> <br /><%= item.details %>
      </li>
      <% }) %>
    </ul>
    <%}%>
  </body>
</html>
